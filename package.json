{
  "name": "20forms-20designs",
  "private": true,
  "version": "1.0.251130.34",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:shell": "npm run dev --workspace=@forms-comparison/shell",
    "build": "node scripts/build-all.mjs",
    "build:shell": "npm run build --workspace=@forms-comparison/shell",
    "build:copy": "node scripts/copy-builds-to-dist.mjs",
    "preview": "node scripts/serve-preview.mjs",
    "preview:shell": "npm run preview --workspace=@forms-comparison/shell",
    "clean": "node scripts/clean.mjs",
    "lint": "eslint 'apps/*/src/**/*.{js,jsx,ts,tsx}' 'packages/*/src/**/*.{js,jsx,ts,tsx}' --cache",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
